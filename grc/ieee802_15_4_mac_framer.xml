<?xml version="1.0"?>
<block>
	<name>MAC Framer</name>
	<key>ieee802_15_4_mac_framer</key>
	<category>IEEE802.15.4</category>
	<import>import gnuradio.ieee802_15_4 as gr_ieee802_15_4</import>
	<make>gr_ieee802_15_4.mac_framer($debug)</make>

	<param>
		<name>Debug</name>
		<key>debug</key>
		<value>False</value>
		<type>bool</type>

		<option>
			<name>Enable</name>
			<key>True</key>
		</option>
		<option>
			<name>Disable</name>
			<key>False</key>
		</option>
	</param>

	<sink>
		<name>in</name>
		<type>message</type>
	</sink>
	<source>
		<name>out</name>
		<type>byte</type>
		<nports>1</nports>
	</source>
</block>
