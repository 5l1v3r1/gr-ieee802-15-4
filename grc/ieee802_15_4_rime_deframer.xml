<?xml version="1.0"?>
<block>
	<name>RIME Deframer</name>
	<key>ieee802_15_4_rime_deframer</key>
	<category>IEEE802.15.4</category>
	<import>import gnuradio.ieee802_15_4 as gr_ieee802_15_4</import>
	<make>gr_ieee802_15_4.rime_deframer($debug)</make>

	<param>
		<name>Debug</name>
		<key>debug</key>
		<value>False</value>
		<type>bool</type>

		<option>
			<name>Enable</name>
			<key>True</key>
		</option>
		<option>
			<name>Disable</name>
			<key>False</key>
		</option>
	</param>

	<sink>
		<name>pdu in</name>
		<type>message</type>
	</sink>

	<source>
		<name>pdu out</name>
		<type>message</type>
	</source>
</block>
